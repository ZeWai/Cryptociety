<link rel="stylesheet" href="../css/{{function}}.css">
<div>
    <div class="sectionleft">
        <div class="image">
            <img src="{{icon}}" id="icon_img">
        </div>
        <h4 class="username">{{username}}</h4>
        <p class="solgan">{{solgan}}</p>
        <form action="/index"><button id="post">Post</button></form>
        <form action="/index/group"><button id="group">Group</button></form>
        <form action="/index/follower"><button id="follower">Follower</button></form>
        <form action="/index/chatroom"><button id="chatroom">Chatroom</button></form>
        <form action="/setting"><button>Setting</button></form>
    </div>
    <div class="sectionright">
        <div id="Post" class="tabcontent">
<section class="title" id="content_list" hidden>
    <div class="row text-center">
        <h3>Content</h3>
        <ul id="content_ul"></ul>
    </div>
</section>
<div id="create_post_btn" class="tabcontent"></div>
<section class="title" id="create_post_section" hidden>
    <div class="row text-center">
        <h3>Create Post</h3>
        <div id="content_wrapper">
            <input class="form-control" type="text" placeholder="Entry something..." id="article">
        </div>
        <div>
            <a class="btn submit col-12" id="article_submit_btn">Submit<i class="fas fa-paper-plane"></i></a>
            <a class="btn cancel" id="article_cancel_btn">Cancel<i class="fas fa-trash-alt"></i></a>
        </div>
    </div>
</section>
        </div>
    </div>
</div>
<script src="https://code.jquery.com/jquery-3.5.0.js"></script>
<script src="https://cdn.socket.io/4.4.0/socket.io.js"></script>
<script src="../js/{{function}}.js"></script>
